  "scripts": {
    "start": "budo --dir build",
    "build": "node script && remark src -qo",
    "lint": "xo && stylelint src/index.css && remark src -q && rehype src -qf",
    "test": "npm run build && npm run lint"
  },
  "dependencies": {
    "in-view": "^0.6.0"
  },
  "xo": {
    "space": true,
    "esnext": false,
    "ignore": [
      "index.js"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "no-descending-specificity": null
    }
  },
  "remarkConfig": {
    "plugins": [
      "./script/markdown/preset-md"
    ]
  },
  "rehype": {
    "plugins": [
      "./script/html/preset"
    ]
  }
}
