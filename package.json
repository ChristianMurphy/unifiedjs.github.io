  "scripts": {
    "start": "budo --dir .",
    "build": "node script && remark src -qo",
    "lint": "xo && stylelint index.css && remark src -qf && rehype src -qf",
    "test": "npm run build && npm run lint"
  },
  "dependencies": {
    "in-view": "^0.6.0"
  },
  "xo": {
    "space": true,
    "esnext": false,
    "ignore": [
      "index.js"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "remarkConfig": {
    "plugins": [
      "./script/markdown/preset-md"
    ]
  },
  "rehype": {
    "plugins": [
      "./script/html/preset"
    ]
  }
