  "scripts": {
    "start": "budo --dir build",
    "format": "remark . -qo && prettier . -w --loglevel warn && stylelint \"**/*.css\" --fix && xo --fix",
    "crawl:sponsors": "mkdir -p data; node crawl/sponsors",
    "crawl:team": "mkdir -p data; node crawl/team",
    "crawl:ecosystem": "mkdir -p data; mkdir -p data/readme; node crawl/ecosystem",
    "crawl": "npm run crawl:sponsors && npm run crawl:team && npm run crawl:ecosystem",
    "generate:asset": "node generate/asset",
    "generate:pages": "node generate/index",
    "generate:theme": "cp node_modules/highlight.js/styles/atom-one-light.css build/syntax-light.css && cp node_modules/highlight.js/styles/atom-one-dark.css build/syntax-dark.css",
    "generate": "npm run generate:asset && npm run generate:pages && npm run generate:theme",
    "test": "npm run format && npm run crawl && npm run generate"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "complexity": "off",
      "unicorn/explicit-length-check": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/no-array-reduce": "off",
      "unicorn/no-reduce": "off",
      "unicorn/prefer-node-remove": "off",
      "unicorn/prefer-optional-catch-binding": "off",
      "unicorn/prefer-set-has": "off"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ],
    "rules": {
      "no-descending-specificity": null
    }
  }
}
